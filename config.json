{
    "distSpecVersion": "1.0.1",
    "http": {
        "address":"0.0.0.0",
        "port":"8080",
        "realm":"zot",
        "auth": {
            "htpasswd": {
                "path": "/etc/zot/htpasswd"
            }
        }
    },
    "storage": {
        "rootDirectory": "/zot",
        "commit": true,
        "dedupe": true,
        "gc": true,
        "gcDelay": "1h",
        "gcInterval": "24h"
    },
    "log": {
        "level": "debug"
    },
    "extensions": {
        "ui": {
            "enable": true
        },
        "search": {
            "enable": true,
            "cve": {
                "updateInterval": "2h"
            }
        }
    }
}